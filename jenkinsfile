pipeline {
    agent any

    stages {
        stage('docker image') {
            steps {
                echo 'build image'
                sh 'sudo docker build -t kat10/proj .'
            }
        }
        stage('docker push') {
            steps {
                withCredentials([usernameColonPassword(credentialsId: 'dockerpush', variable: 'dockerpass')]) {
               sh "sudo docker login -u kat10 -p ${dockerpass}"
               }
                sh 'sudo docker push kat10/proj'
            }
        }

        
    }
}

